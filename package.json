{
  "name": "repository-provider-cli-support",
  "version": "0.0.0-semantic-release",
  "description": "helper functions for cli apps",
  "keywords": [
    "repository-provider"
  ],
  "scripts": {
    "cover": "--overwrite-- c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "commander": "^10.0.1",
    "documentation": "^14.0.1",
    "semantic-release": "^21.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repository-provider-cli-support"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repository-provider-cli-support/issues"
  },
  "homepage": "https://github.com/arlac77/repository-provider-cli-support#readme",
  "template": {
    "inheritFrom": "arlac77/template-esm-only"
  }
}
