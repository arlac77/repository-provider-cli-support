{
  "name": "repository-provider-cli-support",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/setup-provider.mjs"
  },
  "description": "helper functions for repository-provider cli apps",
  "keywords": [
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "aggregation-repository-provider": "^5.3.34",
    "etag-cache-leveldb": "^2.0.3",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "commander": "^11.0.0",
    "documentation": "^14.0.2",
    "github-repository-provider": "^8.0.24",
    "semantic-release": "^22.0.5"
  },
  "engines": {
    "node": ">=16.20.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repository-provider-cli-support"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repository-provider-cli-support/issues"
  },
  "homepage": "https://github.com/arlac77/repository-provider-cli-support#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
